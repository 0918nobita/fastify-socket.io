    "@typescript-eslint/parser": "^3.9.1",
    "fastify": "^3.2.1",
    "standard": "^14.3.4",
    "tap": "^14.10.8",
    "ts-standard": "^9.0.0",
    "tsd": "^0.13.1",
    "typescript": "^3.9.7"
